#set( $pk = "EMPLOYEE" )
#set( $sk = "EMPLOYEE#FIRST_NAME#" )
{
    "version": "2017-02-28",
    "operation": "Query",
    "query": {
        "expression": "PK = :pk AND begins_with(SK, :sk)",
        "expressionValues": {
            ":pk": $util.dynamodb.toDynamoDBJson($pk),
            ":sk": $util.dynamodb.toDynamoDBJson($sk)
        }
    }
}