{
  "api": {
    "pizzaapp": {
      "dependsOn": [
        {
          "attributes": [
            "UserPoolId"
          ],
          "category": "auth",
          "resourceName": "pizzaAuth"
        }
      ],
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [],
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authpizzaAuth"
            }
          }
        }
      },
      "providerPlugin": "awscloudformation",
      "service": "AppSync"
    }
  },
  "auth": {
    "pizzaAuth": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL"
        ],
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    },
    "userPoolGroups": {
      "dependsOn": [
        {
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb",
            "AppClientID",
            "IdentityPoolId"
          ],
          "category": "auth",
          "resourceName": "pizzaAuth"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Cognito-UserPool-Groups"
    }
  },
  "custom": {
    "AppsyncResolvers": {
      "dependsOn": [
        {
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ],
          "category": "api",
          "resourceName": "pizzaapp"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "customCDK"
    },
    "cloudfront": {
      "dependsOn": [
        {
          "attributes": [
            "BucketName",
            "Region"
          ],
          "category": "storage",
          "resourceName": "pizzaStorage"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "customCDK"
    }
  },
  "function": {
    "InvalidateCloudfrontUrls": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "CloudFrontDistributionId"
          ],
          "category": "custom",
          "resourceName": "cloudfront"
        },
        {
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ],
          "category": "api",
          "resourceName": "pizzaapp"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "parameters": {
    "AMPLIFY_function_InvalidateCloudfrontUrls_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "InvalidateCloudfrontUrls"
        }
      ]
    },
    "AMPLIFY_function_InvalidateCloudfrontUrls_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "InvalidateCloudfrontUrls"
        }
      ]
    }
  },
  "storage": {
    "pizzaStorage": {
      "dependsOn": [],
      "providerPlugin": "awscloudformation",
      "service": "S3"
    }
  }
}